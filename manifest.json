{
  "manifest_version": 3,
  "name": "Color Changer",
  "version": "1.0",
  "description": "Change the body color with a button click.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "128": "icons/icon128.png"
  }
}


